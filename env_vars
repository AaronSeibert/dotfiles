# Ansible
export ANSIBLE_HOST_KEY_CHECKING="False"

# Docker
export DOCKER_HOST=ssh://aaron@docker.home.binaryitsystems.com

## Imported from zshrc
# Add RVM to PATH for scripting. Make sure this is the last PATH variable change.
export PATH="$PATH:$HOME/.rvm/bin"

# Vault
export VAULT_ADDR=https://vault-dev.binaryitsystems.com

# NVM
export NVM_DIR="$HOME/.nvm"
[ -s "/opt/homebrew/opt/nvm/nvm.sh" ] && . "/opt/homebrew/opt/nvm/nvm.sh"  # This loads nvm

# kubeconfig
export KUBECONFS=${HOME}/.kube/conf.d
export KUBECONFIG=${KUBECONFS}/corp-rig001-kubeconfig.yaml:${KUBECONFS}/do-nyc1-clients-rig-001-kubeconfig.yaml:${KUBECONFS}/do-nyc1-prod

# Custom Scripts
export PATH="$PATH:$HOME/bin"


export PATH="/usr/local/opt/mysql-client/bin:$PATH"
if command -v pyenv 1>/dev/null 2>&1; then
  eval "$(pyenv init -)"
fi

# openssh
if [ -x "$(brew --prefix)" ]
    export PATH=$(brew --prefix openssh)/bin:$PATH
fi
